{
    "version": "1.0.0",
    "description": "一个基于 GodotHub 开发的 Godot 引擎版本管理工具。",
    "homepage": "https://github.com/morning-start/godot-engine-manager",
    "license": "MIT",
    "url": "https://github.com/morning-start/godot-engine-manager/releases/download/v1.0.0/gdem-x86_64-pc-windows-gnu.zip",
    "hash": "3e5824ab564025d4a61aadc2e5174d77a3af73e0b1fa65f3e4066873f099d349",
    "bin": "gdem.exe",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/morning-start/godot-engine-manager/releases/download/v$version/gdem-x86_64-pc-windows-gnu.zip"
    },
    "env_set": {
        "GDEM_ROOT": "$persist_dir"
    },
